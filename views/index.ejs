<!DOCTYPE html>
<html lang="en">
<!-- home header -->
<%- include('layouts/header') -%>

<body class="bg-layout">
<!-- Navbar -->
<%- include('layouts/navBar') -%>

<!--Section: Design Block-->
<section>
  <!-- Intro -->
  <div id="intro" class="bg-image vh-100" style="
          background-image: url('/img/nurse-staffing-firm.png');
        ">
    <div class="mask" style="background-color: rgba(250, 182, 162, 0.15);">

      <div class="container" style="margin-top: 280px;">

        <div class="row">
          <div data-mdb-toggle="animation"
               data-mdb-animation-start="onLoad"
               data-mdb-animation="fade-in-up"
               data-mdb-animation-delay="300"
               class="col-md-12 text-center">

            <img src="img/logo.png" height="240px;" alt="logo" />

          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- Intro -->
</section>
<!--Section: Design Block-->

<!--<div class="green-logo">-->
<!--  <img src="./img/navLogo.png" style="width:100%;" class="img-fluid max-img rounded-circle" />-->
